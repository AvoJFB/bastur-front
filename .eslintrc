{
  "parser": "babel-eslint",  
  "extends": "airbnb",
  "rules": {
    "no-console": 0,
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "react/prop-types": 0,
    "max-len": [2, 120],
    "import/no-extraneous-dependencies": 0,
    "wrap-iife": 0,
    "react/no-array-index-key": 0,
    "no-confusing-arrow": 0
  },
  "globals": {
    "document": true,
    "window": true
  }
}